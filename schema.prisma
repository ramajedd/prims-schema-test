model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String?
  posts Post[]
}
model Post {
  id        Int      @id @default(autoincrement())
  created   DateTime @default(now())
  title     String   @db.VarChar(70)
  content   String
  author    User?    @relation(fields: [authorId], references: [id])
  authorId  Int
}
